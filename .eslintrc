---

extends:
  - fbjs

rules:
  # this is very noisy, so disable
  consistent-return: 0
  # Prettier and ESLint may disagree on the following rules
  indent: 0
  array-bracket-spacing: 0
  comma-dangle:
    - error
    - arrays: ignore
      objects: ignore
      imports: ignore
      exports: ignore
      functions: never
